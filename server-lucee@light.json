{
    "app":{
        "cfengine":"lucee-light@5",
        "serverHomeDirectory":".engine/lucee-light5"
    },
    "name":"coldbox-lucee-light@5",
    "force":true,
    "openBrowser":false,
    "web":{
        "directoryBrowsing":true,
        "http":{
            "port":"8599"
        },
        "rewrites":{
            "enable":true
        },
        "aliases":{
            "/coldbox":"./"
        }
    },
    "JVM":{
        "heapSize":"1024"
    },
	"cfconfig": {
		"file" : ".cfconfig.json"
	}
}
