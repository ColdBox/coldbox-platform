<cfoutput><script language="javascript">$(document).ready(function() { 	$("##password").focus();});</script><form id="loginform" name="loginform" method="post" action="index.cfm" onSubmit="lon();"><input type="hidden" name="event" value="#Event.getValue("xehLogin")#" /><div>	<!--- Messagebox --->	#getPlugin("messagebox").renderit()#</div><div class="loginbox">	<div class="contentboxes_header">		<div class="contentboxes_title"><img src="images/icons/password_icon.gif" align="absbottom" />&nbsp; Dashboard Login</div>	</div>	<div class="contentboxes">		<div style="margin-left: 20px;margin-right:20px;margin-bottom:30px">		<strong>ColdBox Dashboard Version: #getSetting("version")#</strong>.&nbsp;<br><br>		  Please enter your administrator password below in order to log in. If you have no business here, then LEAVE.		</div>		<div align="center" >			<strong>Password:</strong>&nbsp;			<input name="password" type="password" id="password" size="20" maxlength="30" tabindex="1" />		</div>		<div align="center" style="margin-top:30px">			<a class="action" href="javascript:document.loginform.submit();lon();">				<span>Log In</span>			</a>		</div>		<div class="copyright" align="center" style="margin-top:80px">ColdBox Framework &copy; Copyright #dateFormat(now(),"YYYY")# (<a href="http://www.coldboxframework.com">www.coldboxframework.com</a>)		</div>		<hr size="1"  />	</div></div></form></cfoutput>