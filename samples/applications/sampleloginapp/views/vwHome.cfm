<cfoutput><script language="javascript">function really(){	if ( confirm("Do you really want to logout?") ){		window.location='index.cfm?event=#Event.getValue("xehLogout")#';	}}</script>  <table width="400" align="center" cellpadding="10" cellspacing="0" style="border-bottom:2px solid ##ddd;border-right:2px solid ##ddd; border-left:1px solid ##eaeaea; border-top: 1px solid ##eaeaea;background: ##f5f5f5">    <tr>      <td style="font-family:Georgia, 'Times New Roman', Times, serif; font-size:25px"><strong>Home </strong></td>    </tr>    <!--- ColdBox messages box if errors --->    <cfif not getPlugin("messagebox").isEmpty()>      <tr>        <td>#getPlugin("messagebox").renderit()#</td>      </tr>    </cfif>    <tr>      <td><p>Welcome back #getPlugin("sessionstorage").getVar("name")#!!! </p>          <p>&nbsp;</p>          You have logged in successfully.</td>    </tr>    <tr>      <td align="right"><input type="button" name="button" value="Logout" onclick="really()"/></td>    </tr>  </table></cfoutput><h3 >&nbsp;</h3><p>&nbsp;</p><p>&nbsp;</p>